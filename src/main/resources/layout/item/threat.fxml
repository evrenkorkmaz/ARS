<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>

<HBox alignment="CENTER_LEFT" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefWidth="180.0" spacing="8.0" xmlns="http://javafx.com/javafx/null" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tr.gediz.ars.controller.ThreatItemController">
    <children>
      <ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
         <HBox.margin>
            <Insets />
         </HBox.margin>
         <image>
            <Image url="@../../image/icon/alert.png" />
         </image>
      </ImageView>
        <Label fx:id="name" minWidth="40.0" prefWidth="200.0" text="A-00" HBox.hgrow="ALWAYS">
         <HBox.margin>
            <Insets />
         </HBox.margin></Label>
      <HBox fx:id="interceptButtonsContainer" alignment="CENTER" spacing="8.0">
         <children>
            <ImageView fx:id="planeButton" fitHeight="24.0" fitWidth="24.0" onMouseClicked="#planeButtonClicked" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../image/icon/airplane_takeoff.png" />
               </image>
            </ImageView>
            <ImageView fx:id="missileButton" fitHeight="24.0" fitWidth="24.0" onMouseClicked="#missileButtonClicked" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../image/icon/rocket.png" />
               </image>
            </ImageView>
         </children>
         <HBox.margin>
            <Insets />
         </HBox.margin></HBox>
    </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</HBox>
